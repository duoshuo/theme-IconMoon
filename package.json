{
  "name": "iconmoon",
  "main": "dist/iconmoon.min.css",
  "private" : true,
  "scripts": {
    "serve": "serve --no-less .",
    "build-less": "node_modules/.bin/lessc -x less/iconmoon.less dist/iconmoon.min.css",
    "watch": "rewatch less/*.less -c 'npm run build-less'",
    "dev": "npm run watch & npm run serve",
    "start": "npm run dev"
  },
  "devDependencies": {
    "serve": "~1.4.0",
    "rewatch": "~0.2.2",    
    "less": "~1.7.3"
  }
}